pipeline {
    agent any 
    stages {

        stage('SCM Checkout') {
            steps{
               git 'https://github.com/team6-6/ShelterApp' 
                }
            }
            

    stage ('Prepare'){
      steps {
        sh 'chmod +x ./gradlew'
      }
    }
    
    
    stage('Build'){
            steps{
               sh './gradlew src'
                }
            }
            
      stage('Test'){
            steps{
              echo 'test'
                }
            }
        }
 }
   
